<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <!--
    Documents:https://docs.microsoft.com/zh-tw/visualstudio/msbuild/
    Reference:https://docs.microsoft.com/zh-tw/visualstudio/msbuild/msbuild-reference
    Startup:https://docs.microsoft.com/zh-tw/visualstudio/msbuild/walkthrough-creating-an-msbuild-project-file-from-scratch
    -->
  <Import Project="$(MSBuildExtensionsPath)\Microsoft\VisualStudio\v$(VisualStudioVersion)\TypeScript\Microsoft.TypeScript.props"
    Condition="Exists('$(MSBuildExtensionsPath)\Microsoft\VisualStudio\v$(VisualStudioVersion)\TypeScript\Microsoft.TypeScript.props')"/>
  <Import Project="$(MSBuildExtensionsPath)\Microsoft\VisualStudio\v$(VisualStudioVersion)\TypeScript\Microsoft.TypeScript.targets"
    Condition="Exists('$(MSBuildExtensionsPath)\Microsoft\VisualStudio\v$(VisualStudioVersion)\TypeScript\Microsoft.TypeScript.targets')"/>

  <ItemGroup>
    <ProjectCapability Include="TypeScript"/>
  </ItemGroup>
</Project>