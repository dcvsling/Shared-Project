<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <!--
    Documents:https://docs.microsoft.com/zh-tw/visualstudio/msbuild/
    Reference:https://docs.microsoft.com/zh-tw/visualstudio/msbuild/msbuild-reference
    Startup:https://docs.microsoft.com/zh-tw/visualstudio/msbuild/walkthrough-creating-an-msbuild-project-file-from-scratch
    -->
  <PropertyGroup Condition="$(DockerEnable) == 'true'">
    <DockerComposeProjectPath>$(SharedRoot)Docker\docker-compose.dcproj</DockerComposeProjectPath>
  </PropertyGroup>
  <ItemGroup Condition="$(DockerEnable) == 'true'">
    <ProjectCapability Include="DockerCompose" Condition="$(DockerEnable) == 'true'" />
    <None Include="$(SharedRoot)Docker\*"/>
  </ItemGroup>
</Project>